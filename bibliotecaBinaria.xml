<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bibliotecaBinaria.org" xmlns="http://www.bibliotecaBinaria.org">



    <xsd: element name="biblioteca">
        <xsd: complexType>
            <xsd: sequence>
                <xsd: element ref="codigo" maxOccurs="unbounded" />
            </xsd: sequence>
        </xsd: complexType>
    </xsd: element>
    <xsd: element name="codigo">
        <xsd: complexType>
            <xsd: sequence>
                <xsd: element name="titulo" type="xs:string" />
                <xsd: element name="autor" type="xs:string" />
                <xsd: element name="editorial" type="xs:string" />
                <xsd: element name="edicion" type="xs:string" />
                <xsd: element name="fecha" type="xs:string" />
                <xsd: element name="isbn" type="xs:string" />
                <xsd: element name="numero" type="xs:string" />
            </xsd: sequence>
        </xsd: complexType>
    </xsd: element>



